.hot-list{display: inline-block;white-space: normal;width: 300rpx;height: 192rpx;overflow: hidden;padding: 0 0 0 215rpx;background: #f2f2f2;position: relative;margin-right: 20rpx;border-radius: 6rpx;}
.hot-list .hot-img{position: absolute; top: 0;left: 0;width: 192rpx;height: 192rpx;}
.hot-content .hon-head-img {padding-top: 10rpx;}
.hot-content .hon-head-img image{width: 28rpx;height: 28rpx;margin-right: 10rpx}
.hot-content .hot-goods-name{display:block;text-align: justify;font-size: 24rpx;line-height: 40rpx;height: 80rpx;overflow: hidden;color: #333;}
.hot-content .hot-price{color: red;font-size:30rpx;}
