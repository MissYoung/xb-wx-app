.end-page {
  width: 100%;
  height: 40rpx;
  padding: 10rpx 0 80rpx;
  background: #f2f2f2;
  text-align: center;
  color: #ccc;
  font-size: 28rpx;
  position: relative;
}
.end-page::before {
  content: '';
  display: block;
  position: absolute;
  right: 240rpx;
  top: 32rpx;
  width: 58rpx;
  height: 2rpx;
  background: #ccc;
}
.end-page::after {
  content: '';
  display: block;
  position: absolute;
  left: 240rpx;
  top: 32rpx;
  width: 58rpx;
  height: 2rpx;
  background: #ccc;
}












































































