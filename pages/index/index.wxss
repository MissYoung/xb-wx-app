.selected .title{text-align: center;margin-bottom: 45rpx;}
.selected .title .max-title{font-size: 36rpx;color: #282828;font-weight: bold;line-height: 50rpx;margin-bottom: 10rpx;}
.selected .title .max-title image{width: 36rpx;height: 33rpx;vertical-align: middle;}
