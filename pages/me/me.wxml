<view class='section-four'>
  <view class='sub-item' bindtap='addressTaped'>
    <view class='sub-item-theme'>
      <image class='item-img' src = '/image/UserCenter/item-location.png' ></image>
      <view class='item-name'>收货地址</view>
    </view>
    <image class='arrow' src='/image/UserCenter/arrow.png'></image>
  </view>

  <view class='sub-item' bindtap='collectTaped'>
    <view class='sub-item-theme'>
      <image class='item-img' src = '/image/UserCenter/item-fav.png' ></image>
      <view class='item-name'>我的收藏</view>
    </view>
    <image class='arrow' src='/image/UserCenter/arrow.png'></image>
  </view>

  <view class='sub-item' bindtap='recordsTaped'>
    <view class='sub-item-theme'>
      <image class='item-img' src = '/image/UserCenter/item-time.png' ></image>
      <view class='item-name'>浏览记录</view>
    </view>
    <image class='arrow' src='/image/UserCenter/arrow.png'></image>
  </view>
</view>